<body>
    <h1 id="newHead">New Pokémon Found</h1>

    <form id="newPage" action="/pokedex" method="POST">
        <input type="text" name="name" placeholder="name" /> <br>
        <input type="text" name="type" placeholder="type"/><br>
        <input type="image url" name="img" placeholder="img url"><br>
        <input type="text" name="stats" placeholder="hp"/><br>
        <input type="text" name="stats" placeholder="attack"/><br>
        <input type="text" name="stats" placeholder="defense"/><br>
        <input type="text" name="stats" placeholder="spattack"/><br>
        <input type="text" name="stats" placeholder="spdefense"/><br>
        <input type="text" name="stats" placeholder="speed"/><br>
        <input id="add" type="submit" value="Pokémon Added!">
    
    </form>
